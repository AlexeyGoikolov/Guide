@model Book

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

@if (Model.VirtualPath.Contains(".pdf"))
{
    <iframe width="100%" height="1000" allowfullscreen src="@Model.VirtualPath"></iframe>
}
else
{
    <iframe width="100%" height="1000" src="https://docs.google.com/gview?url=@ViewBag.Path&embedded=true"></iframe>
}
 